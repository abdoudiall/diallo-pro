{
  "name": "diallo-pro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && prettier --check \"src/**/*.{ts,tsx,css,json}\" \"*.{js,json}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\" \"*.{js,json}\"",
    "version:patch": "npm version patch && git push && git push --tags",
    "version:minor": "npm version minor && git push && git push --tags",
    "version:major": "npm version major && git push && git push --tags",
    "version:show": "echo \"Current version: $(git describe --tags --always --dirty)\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@heroicons/react": "^2.1.1",
    "@radix-ui/react-tooltip": "^1.1.8",
    "critters": "^0.0.23",
    "framer-motion": "^10.18.0",
    "next": "^14.2.25",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.2.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
